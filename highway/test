import findspark

findspark.init()

import pyspark as ps

# 创建SparkConf对象
conf = ps.SparkConf().setMaster("local").setAppName("movies")

# 创建SparkContext对象
context = ps.SparkContext(conf=conf)

# 读取文件
highway_RDD = context.textFile("./dataset/201601.tab")

# 分割字符串
parts_highway_RDD = highway_RDD.map(lambda x: x.split(' '))

# 去掉属性名
# highway_RDD = highway_RDD.filter(lambda x: x[0] != "入口网络编号")

print(highway_RDD.first())
